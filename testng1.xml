<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE suite SYSTEM "https://testng.org/testng-1.0.dtd">
<suite name="mysuite2">
	<test name="TestSanity">
		<parameter name="browser" value="chrome"></parameter>
		<parameter name="version" value="121.01.30"></parameter>
		<groups>
			<!--Execute tests with the 'sanity' group -->
			<run>
				<include name="sanity"></include>
			</run>
		</groups>

		<classes>
			<class name="com.salesforce.automation.AutomationScripts_LogIn" />
		</classes>
	</test> <!--
	TestNew -->

	<test name="TestSmoke">
		<classes>
			<class name="com.salesforce.automation.AutomationScripts_LogIn">
				<methods>
					<include name="smoke" />

				</methods>
			</class>
		</classes>
	</test> <!--
	TestNew -->

	<test name="TestRndm">
		<parameter name="browser" value="chrome"></parameter>
		<classes>
			<class name="com.salesforce.automation.AutomationScripts_Random">
				<methods>
					<include name="Random_TC37" />
				</methods>
			</class>
		</classes>
	</test>
</suite>